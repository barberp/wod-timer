{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "wod-timer",
    "version": "1.0.0",
    "orientation": "default",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.barberp.wodtimer",
      "buildNumber": "2",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "This app does not use camera access",
        "NSMicrophoneUsageDescription": "This app does not use microphone access",
        "NSLocationWhenInUseUsageDescription": "This app does not use location access"
      }
    },
    "android": {
      "package": "com.barberp.wodtimer",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#F8F9FA"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7d749cc0-9619-4536-8d84-30b3e37b24e0"
      }
    },
    "owner": "barberp"
  }
}
